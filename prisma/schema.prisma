generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Recipes {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  calories     Int
  description  String
  ingredients  String
  proteinGrams Int
  steps        String
  title        String
  type         String
}
